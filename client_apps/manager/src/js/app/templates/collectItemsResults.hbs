<form action='#'>
<div class="form-group">
  <button class="btn btn-default" type="button">Save selected</button>
</form>
<div class="list-group">
{{#each items}}
<div class="list-group-item media">
  <label for="result-{{@index}}">
  <div  class="pull-left">  
   <input type="checkbox" name="webitem" value="{{@index}}" id="result-{{@index}}" />
    <img src="{{authorAvatarUrl}}"  class="media-object">
  </div>
  </label>

  <div class="media-body">
  <h4 class="media-heading">{{title}}</h4>
  {{{content}}}
  </div>

  <div class="postdata">
    <a href="{{url}}">original</a> posted at {{datePosted}} by <a href="{{authorProfileUrl}}">{{authorName}} ({{usernameID}})</a> on <a href="{{sourceSiteUrl}}">{{sourceSiteName}}</a>
  </div>
</div>
{{/each}}
</div>
</form>
